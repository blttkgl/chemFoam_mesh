FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver            ode;
    method            standard;
}

chemistry       on;
initialChemicalTimeStep 8e-8;

odeCoeffs
{
    solver          seulex;
    absTol          1e-9;
    relTol          1e-5;
}


#include "reactions"


