    Uscaled
    {
        field U;
        create false;
        variables
        (
            "Umax=20;"
        );
        expression "Umax*U/(max(mag(U)))";
        keepPatches true;
        dimension [0 0 0 0 0 0 0];
    }


    Tsmooth
    {
        field T;
        create false;
        variables
